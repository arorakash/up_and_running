# My custom aliases

alias x="exit"
alias myaliases="cat $HOME/.oh-my-zsh/custom/aliases.zsh;"

alias upd="sudo apt update;"
alias lupg="apt list --upgradable;"
alias fupg="sudo apt full-upgrade -y;"
alias armv="sudo apt auto-remove -y;"

alias commover="cvlc /home/aa/Music/sounds/notification/just-saying-593.mp3 --play-and-exit &"

alias share_current='python3 -m http.server 6006;'
alias share_home='python3 -m http.server 6006 -d $HOME;'
